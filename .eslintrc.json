{
  "extends": ["@dvsa/eslint-config-ts", "prettier"],
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "max-len":["error", {
      "code": 150,
      "ignoreComments": true
    }],
    "@typescript-eslint/no-unsafe-enum-comparison": "off"
  }
}
